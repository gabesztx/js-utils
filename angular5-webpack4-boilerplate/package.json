{
  "name": "aot-test",
  "description": "AOT Angular 5 test project",
  "repository": "local",
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && webpack --config build/webpack.config.prod.ts --mode production --progress",
    "dev": "npm run clean && webpack --config build/webpack.config.dev.ts --mode development",
    "watch": "npm run clean && webpack --config build/webpack.config.dev.ts --mode development --watch",
    "start": "npm run clean && concurrently --kill-others \"npm run watch\" \"live-server dist\""
  },
  "dependencies": {
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "angular-svg-icon": "5.0.0",
    "core-js": "2.5.5",
    "rxjs": "5.5.8",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "6.0.0-rc.2.4",
    "@types/angular": "1.6.43",
    "@types/core-js": "0.9.46",
    "@types/node": "9.6.2",
    "@types/rx": "4.1.1",
    "@types/tapable": "1.0.1",
    "@types/webpack": "4.1.3",
    "awesome-typescript-loader": "5.0.0",
    "concurrently": "3.5.1",
    "copy-webpack-plugin": "4.5.1",
    "file-loader": "1.1.11",
    "less": "3.0.1",
    "less-loader": "4.1.0",
    "ngc-webpack": "4.1.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "ts-loader": "4.2.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.8.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  }
}
